		exit 1; \
	fi
	@echo "$(BLUE)\ud83d\udd04 \u0412\u043e\u0441\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d\u0438\u0435 \u0438\u0437 $(BACKUP_DIR)...$(NC)"
